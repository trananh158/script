

game:GetService("GuiService").ErrorMessageChanged:Connect(function()
wait (5) game:GetService("TeleportService"):Teleport(8304191830)
    end);

repeat task.wait(5) until game:IsLoaded()

local args = {
    [1] = "halloween2_event"
}
if game.PlaceId == 8304191830 then
    game:GetService("ReplicatedStorage"):WaitForChild("endpoints"):WaitForChild("client_to_server"):WaitForChild("request_matchmaking"):InvokeServer(unpack(args))
end
queue_on_teleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/trananh158/script/main/join_hallow'))()")
--[[
    pcall(function()
        local exec = tostring(identifyexecutor())
        if exec == "Synapse X" then
            syn.queue_on_teleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/trananh158/script/main/join_hallow'))()")
        elseif exec == "Fluxus" then
            queue_on_teleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/trananh158/script/main/join_hallow'))()")
        elseif exec == "Synapse X" then
            syn.queue_on_teleport("loadstring(game:HttpGet('https://raw.githubusercontent.com/trananh158/script/main/join_hallow'))()")
        end
    end)
]]
